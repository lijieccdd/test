<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jay.test.springboot.mapper.UserMapper">
    <select id="selectByCardNo" resultType="User">
      SELECT * FROM t_user WHERE card_no=#{cardNo}
    </select>
</mapper>